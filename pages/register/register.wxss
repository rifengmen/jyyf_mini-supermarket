/* pages/register/register.wxss */
.logo {
  width: 208rpx;
  height: 208rpx;
  margin: 130rpx auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-flow: column;
}
.logo .logo_img {
  width: 120rpx;
  height: 120rpx;
  margin: 20rpx auto 0 auto;
}
.logo .logo_img image {
  width: 100%;
  height: 100%;
}
.logo .apptitle {
  width: 100%;
  height: 68rpx;
}


.line {
  width: calc(100% - 80rpx);
  height: 90rpx;
  margin: 0 auto 20rpx auto;
}

.line text {
  float: left;
  font-size: 30rpx;
  margin-left: 20rpx;
}

.right {
  float: right;
  border: 1px solid #bebfbf;
  height: 70rpx;
  margin-top: 15rpx;
  width: 65%;
  margin-right: 20rpx;
}

.ipt {
  width: 92%;
  height: 100%;
  margin: 0 auto;
}

.first {
  margin-left: 20rpx;
  margin-right: 40rpx;
}

.checkBox {
  font-size: 30rpx;
  color: #6a6a6b;
  display: flex;
  justify-content: space-between;
}

.btn {
  width: 93%;
  height: 90rpx;
  background: #f57043;
  margin: 30rpx auto;
  line-height: 90rpx;
  text-align: center;
  color: #ffffff;
}

.box {
  width: 93%;
  margin: 0 auto;
  color: #6a6a6b;
  display: flex;
  justify-content: space-between
}

.left {
  float: left;
  text-decoration: underline;
}

.rig {
  float: right;
  text-decoration: underline;
}

.img {
  width: 300rpx;
  margin: 100rpx auto;
  font-size: 30rpx;
  text-align: center;
  color: #6a6a6b;
}

.erweima {
  width: 300rpx;
  height: 300rpx;
}

.floatclear {
  clear: both;
}

.jiechu {
  text-decoration: underline;
}