/* pages/login/login.wxss */
.logo {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 208rpx;
  height: 208rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-flow: column;
}
.logo .logo_img {
  width: 120rpx;
  height: 120rpx;
  margin: 20rpx auto 0 auto;
}
.logo .logo_img image {
  width: 100%;
  height: 100%;
}
.logo .apptitle {
  width: 100%;
  height: 68rpx;
}