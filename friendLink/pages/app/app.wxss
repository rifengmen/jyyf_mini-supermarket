/* friendLink/pages/app/app.wxss */
.app_cont {
  width: 100%;
  height: 100vh;
}
.app_cont .app_image {
  width: 100%;
  height: 100%;
}
.app_btn {
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: calc(100% - 80rpx);
  height: 90rpx;
  line-height: 90rpx;
  margin: 60rpx auto;
}
