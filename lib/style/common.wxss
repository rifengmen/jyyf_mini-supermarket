/* dialog start */
.dialog_cont {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, .7);
}
.dialog {
  width: 540rpx;
  height: 470rpx;
  padding: 40rpx;
}
.dialog_title {
  width: 100%;
  height: 36rpx;
  line-height: 40rpx;
  margin-bottom: 30rpx;
}
.dialog_section {
  width: 100%;
  height: 52rpx;
  line-height: 52rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.dialog_section_tit {
  margin-right: 20rpx;
}
.dialog_section_cont {
  width: 360rpx;
  height: 100%;
}
.dialog_input {
  width: calc(100% - 50rpx);
  height: 46rpx;
  padding: 0 24rpx;
}
.dialog_btn {
  width: calc(100% - 80rpx);
  height: 50rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dialog_cancel {
  width: 173rpx;
  height: 48rpx;
  line-height: 48rpx;
  border: 1px solid #999999;
}
.dialog_confirm {
  width: 173rpx;
  height: 48rpx;
  line-height: 48rpx;
  border: 1px solid #71d793;
}
/* dialog end */
