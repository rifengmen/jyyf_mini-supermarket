  width: 100%;
  height: 200rpx;
}
.logo_img image {
  width: 100%;
  height: 100%;
}
/* logo样式 end */
/* 分类样式 start */
.cate_item {
  width: 20%;
  height: 180rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-flow: column;
}
.cate_img {
  width: 122rpx;
  height: 122rpx;
}
