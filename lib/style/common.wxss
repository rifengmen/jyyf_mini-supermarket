}
/* 分类样式 end */
/* banner图轮播点儿样式 start */
.dots {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-content: center;
}
.dots .dot {
  margin: 0 8rpx;
  width: 10rpx;
  height: 10rpx;
  transition: all .5s;
}
.dots .dot.active{
  width: 24rpx;
}
/* banner图轮播点儿样式 end */
/* 商品列表 start */
.goods_cont {
  width: calc(100% - 88rpx);
  max-height: calc(100vh - 170rpx - 48rpx - 40rpx);
  margin: 0 auto;
  padding: 20rpx;
  overflow-y: scroll;
}
