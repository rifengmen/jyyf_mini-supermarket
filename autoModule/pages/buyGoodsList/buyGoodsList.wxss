/* autoModule/pages/buyGoodsList/buyGoodsList.wxss */
.buy_cont {
  width: 100%;
  height: auto;
  padding: 24rpx 0;
}
.item {
  width: calc(100% - 96rpx);
  height: 180rpx;
  margin: 0 auto 24rpx auto;
  padding: 24rpx;
}
.item view {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
}
.item view:last-child {
  display: flex;
  justify-content: flex-end;
}
.btn {
  width: 160rpx;
  height: 100%;
  line-height: 60rpx;
}

