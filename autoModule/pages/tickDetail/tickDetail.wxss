/* autoModule/pages/tickDetail/tickDetail.wxss */
.tick_cont {
  width: calc(100% - 48rpx);
  height: auto;
  padding: 24rpx 24rpx 200rpx 24rpx;
}
.card {
  width: calc(100% - 40rpx);
  height: 160rpx;
  padding: 0 20rpx;
  box-shadow: 0px 0px 4px 3px rgba(212, 212, 212, 0.5);
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.card .face_value {
  height: 100%;
  line-height: 160rpx;
  margin-right: 20rpx;
}
.card .type {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  flex-flow: column;
}
.section {
  margin: 40rpx 0;
}
.section .tit {
  margin-bottom: 20rpx;
}
